{
  "name": "blue-carbon-nccr-portal",
  "version": 2,
  "framework": "create-react-app",
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "installCommand": "npm install --legacy-peer-deps",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "REACT_APP_BLOCKCHAIN_URL": "https://blue-carbon-blockchain.railway.app",
    "CI": "false"
  }
}
