{
  "name": "blue-carbon-nccr-portal",
  "version": 2,
  "buildCommand": "cd NCCRPortal && npm run build",
  "outputDirectory": "NCCRPortal/build",
  "installCommand": "cd NCCRPortal && npm install",
  "framework": null,
  "functions": {},
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://blue-carbon-blockchain.railway.app/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "REACT_APP_BLOCKCHAIN_URL": "https://blue-carbon-blockchain.railway.app"
  }
}
